@model ServiceFullView
@{
    ViewData["Title"] = Model.PageTitle;
    ViewData["Description"] = Model.MetaDescription;
    IEnumerable<ProfileViewModel> profiles = ViewBag.profiles;
    string cssclass = Model.Id == 16 ? "philipin-nyanya" : Model.Id == 17 ? "philipin-domrabotnica" : null;

}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    @if (ViewData["Description"] != null)
    {
        <meta name="description" content="@ViewData["Description"]" />
    }
    <link rel="icon" href="~/images/Main/favicon.ico" type="image/x-icon">
    <environment include="Development">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/4.1.1/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/css/site.css" />
        <link href="~/lib/material-design-iconic-font/dist/css/material-design-iconic-font.css" rel="stylesheet" />
        <link href="~/css/slick.css" rel="stylesheet" />
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
        <link href="~/css/slick.css" rel="stylesheet" />
        <link href="~/css/menu.css" rel="stylesheet" />
        <link href="~/css/ladda.min.css" rel="stylesheet" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/4.1.1/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
        <link href="~/lib/material-design-iconic-font/dist/css/material-design-iconic-font.css" rel="stylesheet" />
        <link href="~/css/slick.css" rel="stylesheet" />
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
        <link href="~/css/slick.css" rel="stylesheet" />
        <link href="~/css/menu.css" rel="stylesheet" />
        <link href="~/css/ladda.min.css" rel="stylesheet" />
    </environment>
</head>
<body>
    @{
        Layout = null;
    }
    <div id="dialogDiv" class="modal modal-primary fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div id="dialogContent" class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">

                    <h3 class="modal-title text-center" id="myModalLabel"></h3>
                    <button type="button" class="close float-right pl-0 pr-0 ml-0 mr-0" id="close" data-dismiss="modal" aria-hidden="true">×</button>

                </div>
            </div>
        </div>
    </div>
    <div class="phillipinsmainheader width100">
        <div id="left-flyout-nav" class="layout-left-flyout visible-sm"></div>

        <div class="mainheaderhover">
            <header class="the-header forsmallscreenshow">
                <div class="navbar container ">
                    <div class="row ml_10">
                        <div class="col-1">
                            <a class="btn-navbar btn-navbar-navtoggle" href="#">
                                <span class="icon-bar btn-flyout-trigger"></span>
                                <span class="icon-bar btn-flyout-trigger"></span>
                                <span class="icon-bar btn-flyout-trigger"></span>
                            </a>
                        </div>
                        <div class="col-4 mt-1">
                            <a asp-area="" asp-controller="Home" asp-action="Index" class="ml-4">
                                <img src="~/images/logo.png" class="w-100" />
                            </a>
                        </div>
                        <div class="col-6 mt-1">
                            <a asp-controller="Order" asp-action="Call" class="w-100 btn btn-red text-white modalform ml-3 pl-0 pr-0" onclick="ga('send', 'event', 'form', 'click','feedback');">Заказать звонок</a>
                        </div>
                    </div>
                    <nav class="the-nav nav-collapse clearfix">
                        <ul class="nav nav-pill ml-3 mt-2 bg-transparent">
                            <li class="mt-5">
                                <a asp-area="" asp-controller="Home" asp-action="Index" class="nav-item nav-link navbar-brand ml-5">
                                    <img src="~/images/logo.png" class="menulogo" />
                                </a>
                                <a class="nav-item nav-link text-white fontsize14 ml-5 " asp-route="ServiceIndex">Услуги и цены</a>
                                <a class="nav-item nav-link text-white fontsize14 ml-5" asp-route="Order">Заказать домашний персонал</a>
                                <a class="nav-item nav-link text-white fontsize14 ml-5" asp-route="Applicants">Соискателям</a>
                                <a class="nav-item nav-link text-white fontsize14 ml-5" asp-route="About">Об агенстве</a>
                                <a class="nav-item nav-link text-white fontsize14 ml-5" asp-route="Contacts">Контакты</a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </header>
            <nav id="mainNav" class="navbar forbgtransparent navbar-expand-lg  body-content container-fluid bg-transparent forbigscreenshow">
                <div class="">

                    <div class="forfootermp mt_60 text-md-center">
                        <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand d-inline-block ml-lg-4 logomr pr-lg-5 mt-lg-2 mb-lg-2">
                            <img src="~/images/logo.png" class="logo forbtnm forlogomtml ml15rem mt05rem" />
                        </a>
                        <p class="text-white forshowonbigscreen d-inline-block ml1_4 mr-5 headerlogo mt-2 mb-2">
                            <i class="fa fa-envelope"></i> info@amalco-agency.ru
                            @*<br /><a href="https://wa.me/79266100426" class="text-white">Напишите нам в WhatsApp</a>*@
                        </p>
                        <p class="text-white forshowonbigscreen d-inline-block ml1_4 mr-5 headerlogo mt-2 mb-2"><i class="fa fa-phone "></i>  <i class="fab fa-whatsapp pl-2"></i><i class="fab fa-viber pl-2"></i> <a class="phonenumber" href="tel:+74955457568" onclick="gtag('event', 'click', { 'event_category': 'form', 'event_label': 'PhoneNumber' })">+74955457568</a></p>
                        <a asp-controller="Order" asp-action="Call" class="btn btn-red text-white forbuttonml mt_250  mb-2 modalform">Заказать звонок</a>
                    </div>
                    <div class="pl-4">
                        <div class="forsmmlpl">
                            <div class="navbar-nav forsmmlpl mb-md-4 mb-mt-2">
                                <a class="text-white fontsize14 ml-lg-1 mr-lg-5 ml-md-2 mr-md-2 ml1rem" asp-route="ServiceIndex">Услуги и цены</a>
                                <a class="text-white fontsize14 ml-lg-5 mr-lg-5 ml-md-2 mr-md-2 mr-3" asp-route="Order">Заказать домашний персонал</a>
                                <a class="text-white fontsize14 ml-lg-5 mr-lg-5 ml-md-2 mr-md-2 mr-3" asp-route="Applicants">Соискателям</a>
                                <a class="text-white fontsize14 ml-lg-5 mr-lg-5 ml-md-2 mr-md-2 mr-3" asp-route="About">Об агенстве</a>
                                <a class="text-white fontsize14 ml-lg-5 mr-lg-5 ml-md-2 mr-md-2 mr-3" asp-route="Contacts">Контакты</a>
                            </div>
                        </div>
                    </div>
                </div>
            </nav>

            <div class="row text-white width100">
                <div class="col-12 ml-2 text-center">
                    <div class="forfootermp forsmallscreenshow">
                        <div class="row">
                            <div class="col-12 w-100 text-center pl-0 pr-0 mt-3">
                                <p class="text-white">
                                    <i class="fa fa-envelope"></i> info@amalco-agency.ru
                                    @*<br /><a href="https://wa.me/79266100426" class="text-white">Напишите нам в WhatsApp</a>*@
                                </p>
                            </div>
                            <div class="col-12 w-100 text-center">
                                <p class="text-white"><i class="fa fa-phone "></i>  <i class="fab fa-whatsapp pl-2"></i><i class="fab fa-viber pl-2"></i> <a class="phonenumber" href="tel:+74955457568" onclick="gtag('event', 'click', { 'event_category': 'form', 'event_label': 'PhoneNumber' })">+74955457568</a></p>
                            </div>
                        </div>
                    </div>
                    <p class="fontsize25">Агентство по подбору домашнего персонала</p>
                    @*<p>Неорганиченное количество замен подобранного персонала</p>*@
                </div>
            </div>
            <div class="pt-5 pb-5 width100">
                <div class="offset-lg-3 col-lg-6 offset-md-2 col-md-8 offset-1 col-10 esim">
                    <div class="row">
                        <h3 class="col-md-12 col-12 text-center pb-3 mt-3 text-white">Онлайн-заказ персонала</h3>
                        <div class="mb-3">
                            <div class="col-12">
                                <div class="fororderlefts">
                                    @await Html.PartialAsync("_Order")
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row mt-5 width100">
        <div class="offset-lg-2 col-lg-3 offset-md-1 col-md-4 offset-1">
            @await Component.InvokeAsync("_Menu")
        </div>
        <div class="col-lg-5 offset-lg-0 col-md-6  offset-1 col-11">
            <h1 class="text-center  menuhead forsmpl">@Model.HeaderTitle</h1>
            <div class="forbigml forimg">
                @Html.Raw(Model.Content)
            </div>
        </div>
    </div>

    <div class="mainmiddle mt-5 width100">
        <div class="esim">
            <div class="row pt-5 pb-5 width100 mr-0 ml-0">
                <h3 class="text-white text-center col-md-12">Наши преимущества</h3>
                <div class="offset-lg-2 col-lg-2 col-md-3 col-12 mb-2 mt-5">
                    <div class="row">
                        <div class="col-12 text-center mb-4">
                            <img src="~/images/Icons/001-thumbs-up.png" class="middleicons  text-center" />
                        </div>
                        <div class="middletext col-12 text-center">
                            <h5 class="text-white">10 лет на рынке домашнего персонала</h5>
                            <img src="~/images/Icons/line.png" class="mb-2 w-75" />
                            <p class="text-white fontsize14 fortextpaddings">10 лет в сфере кадровых услуг и более 6-и лет по отбору домашнего персонала из стран Азии</p>
                        </div>

                    </div>
                </div>
                <div class="col-lg-2 col-md-3  col-12 mb-2 mt-5">
                    <div class="row">
                        <div class="col-12 text-center mb-4">
                            <img src="~/images/Icons/002-team.png" class="middleicons text-center fsm" />
                        </div>
                        <div class="middletext col-12 text-center">
                            <h5 class="text-white ">Персонал <br /> из <br />Филиппин</h5>
                            <img src="~/images/Icons/line.png" class="mb-2 w-75" />
                            <p class="text-white fontsize14 fortextpaddings">У всех наших нянь и домработниц из Филиппин есть рабочие визы, разрешение на работу и регистрация</p>
                        </div>
                    </div>

                </div>
                <div class="col-lg-2 col-md-3 col-12 mb-2 mt-5">
                    <div class="row">
                        <div class="col-12 text-center mb-4">
                            <img src="~/images/Icons/003-world.png" class="middleicons text-center fsm" />
                        </div>
                        <div class="middletext col-12 text-center">
                            <h5 class="text-white">Партнерские соглашения  в 5 странах</h5>
                            <img src="~/images/Icons/line.png" class="mb-2 w-75" />
                            <p class="text-white fontsize14 fortextpaddings">Агентство представлено в следующих странах: Россия, Украина, Израиль, Китай и Сингапур</p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-12 mt-5">
                    <div class="row">
                        <div class="col-12 text-center mb-4">
                            <img src="~/images/Icons/004-interview.png" class="middleicons text-center fsm" />
                        </div>
                        <div class="middletext col-12 text-center">
                            <h5 class="text-white">Бесплатный  <br />выезд <br /> менеджера</h5>
                            <img src="~/images/Icons/line.png" class="mb-2 w-75 " />
                            <p class="text-white fontsize14 fortextpaddings">Для вашего комфорта наши менеджеры готовы выехать в любую точку Москвы и Московской области</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row mt-5 mb-5 width100">
        <h3 class="col-md-12 text-center  mb-5 col-12">Наши цены</h3>
        @await Component.InvokeAsync("_ServiceList", true)
    </div>
    @if (profiles.Any())
    {
        <div class="container">
            <div class="row mt-5 ">
                <h4 class="text-blue col-md-12 text-center mb-5">Анкеты соискателей</h4>
            </div>




            @foreach (var item in profiles)
            {
                <div class="row mb-3 border-bottom pb-3">
                    <div class="col-md-2 col-4">
                        <img src="http://profiles.amalco.ru/images/@item.Image" class="w-100" />
                    </div>
                    <div class="col-md-10 col-8 text-justify">
                        <h5 class="text-blue">@item.FirstNae</h5>

                        <p class="fontsize11 pb-1">
                            @item.Text
                        </p>
                        <p class="fontsize10">Опыт работы: @item.Expirence</p>
                        <a href="http://profiles.amalco.ru/profile/@ViewBag.url/@item.Id" class="border-bottom fontsize10 text-black" target="_blank">Посмотреть анкету</a>
                    </div>
                </div>
            }
        </div>
    }
    @await Component.InvokeAsync("_ReviewList")
    <div class="pt-5 pb-5 width100">
        <div class="offset-md-3 col-md-6 offset-1 col-10">
            <div class="row">
                <h3 class="col-md-12 col-12 text-center pb-3 mt-3">Онлайн-заказ персонала</h3>
                <div class="mb-3">
                    <div class="col-12">
                        <div class="fororderlefts">
                            @await Html.PartialAsync("_Order")
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <footer class="bg-blue">
        <div class="container">
            <div class="row ">
                <div class="forfootermp pt-3">
                    <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand pt-4 d-inline-blockb">
                        <img src="~/images/logo.png" class="footerlogo ml15rem" />
                    </a>
                    <a class="nav-item nav-link text-white fontsize14 ml-2 mr-2 footermenu" asp-route="ServiceIndex">Услуги и цены</a>
                    <a class="nav-item nav-link text-white fontsize14 ml-2 mr-2 footermenu" asp-route="Order">Заказать домашний персонал</a>
                    <a class="nav-item nav-link text-white fontsize14 ml-2 mr-2 footermenu" asp-route="Applicants">Соискателям</a>
                    <a class="nav-item nav-link text-white fontsize14 ml-2 mr-2 footermenu" asp-route="About">Об агенстве</a>
                    <a class="nav-item nav-link text-white fontsize14 ml-2 mr-2 footermenu" asp-route="Contacts">Контакты</a>
                </div>
            </div>
        </div>

        <div class="mr-lg-5 pr-lg-3 pt-3">
            <div class="container">
                <div class="row forfootermp">
                    <a class="nav-item nav-link text-white fontsize14 ml-3 ml-md-0 mr-3 mt-3 d-inline-block col-lg-3 col-md-11 col-11" href="#">Кадровое агентство Амалко - подбор домашнего персонала в Москве</a>
                    <a class="nav-item nav-link text-white fontsize14 ml-3 ml-md-0 mr-3 mt-3 d-inline-block col-lg-3 col-md-11 col-11" href="#">Россия, Московская область, Москва, Электрический переулок 3/10, строение 3, 3 подъезд  </a>
                    <div class="col-lg-3 col-md-11 col-11">
                        <div class="row">
                            <p class="text-white fontsize14 d-inline-block ml-3 ml-md-0 ml-lg-3 mt-3 logomr mb-0 col-lg-12 col-md-5 col-12"><i class="fa fa-phone "></i> <a class="phonenumber" href="tel:+74955457568" onclick="gtag('event', 'PhoneNumber', { 'event_category': 'form', 'event_action': 'click' })">+74955457568</a></p>
                            <p class="text-white fontsize14 d-inline-block ml-3 logomr col-lg-12 col-md-6 col-12 mt-3"><i class="fa fa-envelope"></i> info@amalco-agency.ru</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="border-top">
            <div class="mlmr5 pl1rem mt-3">
                <div class="container">
                    <div class="row pl-lg-5 ml-lg-2">
                        <p class="col-lg-6 col-6 textCenter text-white fontsize14 d-inline-block ml-3">Copyright 2014-2018 / Амалко.</p>
                        <p class="col-lg-3 offset-lg-0 col-5 textCenter text-white fontsize14 d-inline-block">
                            <a href="https://www.facebook.com/amalco.ru/" class="text-white"><i class="fab fa-facebook-f "></i></a>
                            <a class="pl-3 text-white" href="https://instagram.com/nanny_agency_amalco"><i class="fab fa-instagram"></i></a>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </footer>


    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.3.1.js"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/4.1.1/bootstrap.js"></script>

    <script src="~/js/jquery.maskedinput.js"></script>
    <script src="~/js/modalform.js"></script>
    <script src="~/js/slick.min.js"></script>
    <script src="~/js/spin.min.js"></script>
    <script src="~/js/ladda.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/menu.js"></script>
    @await Html.PartialAsync("_Counters")
</body>
</html>
